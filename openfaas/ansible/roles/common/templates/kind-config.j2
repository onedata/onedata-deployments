kind: Cluster
apiVersion: kind.x-k8s.io/v1alpha4
name: {{kind_cluster_name}}
nodes:
- role: control-plane
  extraPortMappings:
{% for i in range(kind_number_of_ports) %}
  {{('- containerPort: ' ~ ((kind_first_node_port+i)|string))}}
  {{('  hostPort: ' ~ ((kind_first_port+i)|string))}}
{% endfor %}
